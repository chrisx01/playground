<apex:page standardController="Store__c" showHeader="false" sidebar="false" standardStylesheets="false">
<head>
<link rel="stylesheet" href="http://code.jquery.com/mobile/1.0.1/jquery.mobile-1.0.1.min.css" />
<script src="http://code.jquery.com/jquery-1.6.4.min.js"></script>
<script src="http://code.jquery.com/mobile/1.0.1/jquery.mobile-1.0.1.min.js"></script>

<script src="http://code.jquery.com/ui/1.8.23/jquery-ui.min.js" type="text/javascript"></script>
<link rel="stylesheet" href="http://code.jquery.com/ui/1.8.23/themes/base/jquery-ui.css" type="text/css" media="all" />

    <script>
        jQuery.noConflict();
    </script>

</head>

<body>
<div data-role="page" data-theme="b">
    <div data-role="header" data-add-back-btn="false">
        <a href="/apex/simplelisttouch" data-icon="home" data-iconpos="notext" data-role="button" data-theme="e" data-transition="fade"/><br/>
         &nbsp;
    </div>

    <div data-role="content">

        <ul data-role="listview" data-inset="true" data-theme="b" data-dividertheme="b">
          <li data-role="list-divider">Store Details</li>
            <li>Storename: <apex:outputField value="{!Store__c.StoreName__c}"/></li>
            <li>Store Type: <apex:outputField value="{!Store__c.StoreType__c}"/></li>
            <li>website: <apex:outputField value="{!Store__c.url__c}"/></li>
            <li>location: <apex:outputField value="{!Store__c.Location__c}"/></li>
        </ul> 

     </div>

     <div data-role="footer" >
         <a href="/apex/simplelisttouch" data-icon="arrow-l" data-iconpos="notext" data-role="button" data-theme="e" class="ui-btn-left"/>
         <a href="/apex/simplelisttouch" data-icon="arrow-r" data-iconpos="notext" data-role="button" data-theme="e" class="ui-btn-right"/>

    </div>

</div>
</body>
</apex:page>